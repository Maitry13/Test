<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
		<servlet-name>simpleServlet</servlet-name>
		<servlet-class>com.SimpleServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
  <servlet>
  	<servlet-name>LdapServiceServlet</servlet-name>
  	<display-name>LdapServiceServlet</display-name>
  	<description></description>
  	<servlet-class>com.ldap.LdapServiceServlet</servlet-class>
  </servlet>
	
	<servlet-mapping>
		<servlet-name>simpleServlet</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LdapServiceServlet</servlet-name>
		<url-pattern>/LdapServiceServlet</url-pattern>
	</servlet-mapping>
  
</web-app>
